public service Function_Calls::Big_Object.Have_EIGHT_Same_IH_Parameters (Param1        : in  instance of Object_One,
                                                                         Param2        : in  instance of Object_One,
                                                                         Param3        : in  instance of Object_One,
                                                                         Param4        : in  instance of Object_One,
                                                                         Param5        : in  instance of Object_One,
                                                                         Param6        : in  instance of Object_One,
                                                                         Param7        : in  instance of Object_One,
                                                                         Param8        : in  instance of Object_One,
                                                                         Has_It_Passed : out boolean,
                                                                         What_With     : out integer) is
if Param1.Attribute_One  = 1 &\
   Param2.Attribute_One  = 2 &\
   Param3.Attribute_One  = 3 &\
   Param4.Attribute_One  = 4 &\
   Param5.Attribute_One  = 5 &\
   Param6.Attribute_One  = 6 &\
   Param7.Attribute_One  = 7 &\

      # Check counter values
      if (countA != 1)|(countB != 1)|(countC != 1) then
         [] = RPT3:Test_Failed["objFM", Local_Test, -6]
         Has_Failed = TRUE
      else
         temp = an_instanceFM.ResultFM + 10
         an_instanceFM.ResultFM = temp
      endif

      if !Has_Failed then
         [] = RPT2:Test_Passed["objFM", Local_Test, temp]
      endif


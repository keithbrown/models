   # correct instances
   if countof {setA} != 3 then
      [] = RPT3:Test_Failed["objFM", Local_Test, -4]
      Has_Failed = TRUE
   else
      # Check each instance that is returned
      countA = 0
      countB = 0
      countC = 0

